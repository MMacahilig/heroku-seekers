{% if banner_url != '' %}
<div id="banner_div" class='banner visible-lg visible-md'>
  <img class='img-responsive' src='http://119.9.8.33/sites/533a6766f92ea180dd000002/theme/images/cs-banner-3.png'>
</div>
{% endif %}
<style>
    .youtube-player { display: block; margin: 0 auto; }
</style>
<div class='container-fluid'>


	{% editable_text content %}

	{% endeditable_text %}
