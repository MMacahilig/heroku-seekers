{% include header2 %}
{% block main %}

<div class='container-fluid'>
  {% include page_header %}
  
  <div class='row bottom-margin-large'>
    {% for board in contents.board %}
    <div class="col-lg-12" style="margin-bottom: 50px">
        <div class='col-lg-4'>
          <img alt='{{board.name | escape}}' class='' src='{{board.image | escape}}' style="width: 350px;height: 350px">
        </div>
        <div class='col-lg-8'>
          <h3>{{board.name | escape}}</h3>
          <h4 class='profile-position'>{{board.position | escape}}</h4>
          <p class='profile-description'>
            {{board.description}}
          </p>
          
        </div>
    </div>
      
      
    {% endfor %}
  </div>
  
</div>
{% endblock %}




