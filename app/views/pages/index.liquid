        {% include header %}

        {% block main %}

        {% capture 'main_image' %}{% editable_file 'main_image' %}{% endeditable_file %}{% endcapture %}
        {% capture 'mobile_image' %}{% editable_file 'mobile_image' %}{% endeditable_file %}{% endcapture %}
        {% capture 'mid_career_image' %}{% editable_file 'mid_career_image' %}{% endeditable_file %}{% endcapture %}
        {% capture 'employer_image' %}{% editable_file 'employer_image' %}{% endeditable_file %}{% endcapture %}

        {% assign flash_message = session.flash.message%}
        {% case flash_message['status'] %}
        {% when 'success' %}
        <div class='alert alert-success alert-dismissable'>
            <button aria-hidden='true' class='close' data-dismiss='alert' type='button'>×</button>
            <strong>Congratulations!</strong>
            {{flash_message['message']}}
        </div>
        {% when 'error' %}
        <div class='alert alert-danger alert-dismissable'>
            <button aria-hidden='true' class='close' data-dismiss='alert' type='button'>×</button>
            <strong>Error!</strong>
            {{flash_message['message']}}
        </div>
        {% endcase %}

        
        <div class='container-fluid' style="padding-left:0;padding-right:0;">

            <div id="poster_div" class="poster hidden-md-down" style="padding-left:0;padding-right:0;">
                <img class='img-responsive' src='http://119.9.8.33/sites/533a6766f92ea180dd000002/theme/images/cs-demo-poster-grey-2.png'>
            </div>


        </div>
        <!--
        <div class='container-fluid' style="padding-left:0;padding-right:0;">
            <div class="poster visible-xs" style="padding-left:0;padding-right:0;">
                <img class='img-responsive' src='http://119.9.8.33/sites/533a6766f92ea180dd000002/theme/images/mobile_front_page_banner.png'>
            </div>
        </div>
        -->
        <div class='container marketing'>
            <!--  <div class='page-header container text-center white-background'>-->
            <div class='lead'style="text-align:center">
                {% editable_text 'stories' %}

                <h3>
                    Click on the videos to hear our participants share their stories
                </h3>
                {% endeditable_text %}
            </div>
            <div class='lead'style="text-align:center">

                <h3>
                    2017
                </h3>

            </div>

            <div class='row'>
                {% for story in contents.2017_stories %}
                <div class='col-lg-4'>
                    <a href="" data-target="#{{story._slug}}-modal" data-toggle='modal'><img alt='{{story.student_name | escape }}' src='{{story.url | escape }}' ></a>
                    <h3 style="line-height:0.2em;">  {{story.student_name | escape }}</h3>
                    <p>  {{story.origin | escape }}</p>
                    <b>{{story.participant_stream | escape }}</b>
                    <p>{{story.company | escape }}</p>


                    <div aria-hidden='true' class='modal fade' id='{{story._slug}}-modal' role='dialog' tabindex='-1'>
                        <div class='modal-dialog'>
                            <div class='modal-content'>
                                <div class='modal-header'>
                                    <button aria-hidden='true' class='close' data-dismiss='modal' type='button'>×</button>
                                    <h2 class='modal-title' id='myModalLabel'>{{story.student_name}}</h2>
                                </div>
                                <div class='modal-body'>
                                    <div class='player' id='{{story._slug}}-video' src='{{story.youtube_video_url | escape}}'></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                {% endfor %}
            </div>

        </div>


        {% endblock %}
        {% include footer %}
